class shark:
    
    def __init__(self,name):
        self.name = name

    def swim(self):
            print(self.name + " is swimming.")

    def be_awesome(self):
            print(self.name + " is being awesome")

def main():
    #set name of shark object

    sammy = shark("sammy")
    sammy.swim()
    sammy.be_awesome()
    stevie = shark("steive")
    stevie.swim()

if __name__ == "__main__":
    main()

            
